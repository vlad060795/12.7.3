money = int(input("Введите сумму депозита:"))

per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0} 
bank1 = per_cent['ТКБ']
deposit1= round(bank1/100*money)


bank2 = per_cent['СКБ']
deposit2= round(bank2/100*money)


bank3 = per_cent['ВТБ']
deposit3= round(bank3/100*money)


bank4 = per_cent['СБЕР']
deposit4= round(bank4/100*money)


deposit = [deposit1, deposit2, deposit3, deposit4]
print(f"Вашb депозитs в различных банках:{deposit}.")

max_deposit = max(deposit)

print(f"Ваш максимально прибыльный депозит: {max_deposit}!") 
